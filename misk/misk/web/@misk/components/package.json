{
  "name": "@misk/components",
  "version": "0.0.90",
  "license": "SEE LICENSE IN https://github.com/square/misk",
  "description": "Microservice Kontainer Common Components",
  "author": "Square/Misk Authors (https://github.com/square/misk/graphs/contributors)",
  "main": "lib/web/@misk/components/components.js",
  "module": "lib/web/@misk/components/components.js",
  "typings": "lib/web/@misk/components/index.d.ts",
  "engines": {
    "node": ">=7.8.0"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:square/misk.git"
  },
  "scripts": {
    "build": "yarn run lint && yarn run lib",
    "clean": "rm -rf lib",
    "dev": "yarn run build; yarn run updateDevCache",
    "ci-build": "yarn run clean && yarn run lib",
    "lib": "webpack",
    "lint": "prettier --write --config prettier.config.js \".{/src/**/,/}*.{md,css,sass,less,json,js,jsx,ts,tsx}\"",
    "prepare": "cd ../../../../ && ./gradlew web -Pcmd='-a' -Ptabs='@misk/components'",
    "reinstall": "rm -rf node_modules && yarn install",
    "start": "yarn run lint && cross-env NODE_ENV=development webpack-dev-server",
    "test": "echo no tests",
    "updateDevCache": "cd ..; ./updateLocalCache.sh"
  },
  "dependencies": {
    "@misk/common": "^0.0.70"
  },
  "devDependencies": {
    "@misk/dev": "^0.0.69",
    "@misk/tslint": "^0.0.10"
  },
  "miskTab": {
    "version": "0.0.7"
  }
}
