{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:latest", 
    "tslint-blueprint", 
    "tslint-clean-code", 
    "tslint-config-prettier", 
    "tslint-consistent-codestyle",
    "tslint-eslint-rules", 
    "tslint-immutable",
    "tslint-react",
    "tslint-sonarts"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "always-prefix"],
    "semicolon": [true, "never"],
    "member-access": [false],
    "ordered-imports": [true],
    "no-console": [false],
    "no-var-requires": true,
    "no-implicit-dependencies": false,
    "quotemark": [true, "double", "jsx-double", "avoid-template", "avoid-escape"]
  },
  "rulesDirectory": []
}
